models/items/flashlight/flare3_red {
    translucent
   deform sprite
   {
      if ( parm6 == 1 )
      blend         add
	map		lights/biground1.tga
		red 		( blinktable2 [Parm4 + (time * (4 * Parm3)) ]) * Parm0
		green 		( blinktable2 [Parm4 + (time * (4 * Parm3)) ]) * Parm1
		blue 		( blinktable2 [Parm4 + (time * (4 * Parm3)) ]) * Parm2
      map            models/items/flashlight/flare3_red.tga
   }
}

