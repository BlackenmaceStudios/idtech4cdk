models/mapobjects/fpos/fpos

{
		noselfshadow
	renderbump  -size 512 512 -aa 2 -trace 0.05 -colorMap models/mapobjects/fpos/fpos_local.tga models/mapobjects/fpos/fpos_hi.lwo


// blended part
	{
		blend 		bumpmap
		map		addnormals (models/mapobjects/fpos/fpos_local.tga, heightmap (models/mapobjects/fpos/fpos_h.tga, 5) ) 
	// note that you do NOT put vertexColor on the bumpmap
	}
	{
		blend 		diffusemap
		map		models/mapobjects/fpos/fpos.tga
		rgb		(sinTable [(time*.2)]+1)*0.5
		//alphaTest .5 + 0.5 * sintable [ time * .1  ]
	}
	{
		blend		specularmap
		map		models/mapobjects/fpos/fpos_s.tga
		rgb		(sinTable [(time*.2)]+1)*0.5
	}
// inverse blended part
	{
		blend		bumpmap
		map		addnormals (models/mapobjects/fpos/fpos_local.tga, heightmap (models/mapobjects/fpos/fpos2_h.tga, 5) ) 
	}
	{
		blend		diffusemap
		map		models/mapobjects/fpos/fpos2.tga
		rgb		1-(sinTable [(time*.2)]+1)*0.5
		//alphaTest .5 + 0.5 * sintable [ time * .1  ]
	}
	{
		blend 		specularmap
		map		models/mapobjects/fpos/fpos2_s.tga
		rgb		1-(sinTable [(time*.2)]+1)*0.5
	}



}

