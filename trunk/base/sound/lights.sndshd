// ----------------------------------
// Break sound with silence following
//
// Use this as an altSound on a light shader if you want to shoot it, and have the light
// go silent with no flickering after it's broken.
// ----------------------------------

light_break {
	minDistance		5
	maxDistance		25
	volume			5						// This should be played louder so it's heard
											// over weapon effects and whatnot.
	unclamped


	leadin sound/lights/1shot_lightbreak_01.wav
}


