windowDef Desktop
{
	rect	0, 0, 640, 480
	backcolor	0, 0, 0, 0
	visible	1
	menugui	1
	// switches to != 0 to setup a voting value type, processed in onEvent
	// 1 - text message
	// 2 - integer
	// 99 - hide all value type (used in onActivate, and when cancelling vote)
	float	"valueType"	0
	float	"localVoteIndex"	0
/*
input:
gui::callvote == 1: at least one vote is on
gui::callvote == 0: all votes are off
gui::callvote == 2: there is an active vote
gui::vote{0,1,2,..} == 1: this vote is enabled
                    == 0: this vote is disabled
gui::si_*: all serverinfo cvars are mapped into the gui dict
gui::kickChoices: entries for player kicking choiceDef ( <clientNum> - <nickName> )

output:
gui::voteIndex: the vote selected
  note that you can't do == tests on gui:: variables. so we use a float localVoteIndex
gui::str_voteValue: the corresponding value voted for
  note the str_ prefix, to force a string type internally, otherwise you get a float
*/
	onActivate {
		setFocus "chat" ;
		//	resetTime "BoxBG" "0" ;
		set "BoxParent1::visible" "1" ;
		set "BoxParent2::visible" "0" ;
		set "BoxParent3::visible" "0" ;
		set "BoxParent4::visible" "0" ;
		set "BoxParent5::visible" "0" ;
		set "BoxParent6::visible" "0" ;
		set "BoxParent7::visible" "0" ;
		
		set "VoteTypeRestart::backcolor" "0.6 1 1 0" ;
		set "VoteTypeNextMap::backcolor" "0.6 1 1 0" ;
		set "VoteTypeTimeLimit::backcolor" "0.6 1 1 0" ;
		set "VoteTypeFragLimit::backcolor" "0.6 1 1 0" ;
		set "VoteTypeGameType::backcolor" "0.6 1 1 0" ;
		set "VoteTypeMap::backcolor" "0.6 1 1 0" ;
		set "VoteTypeSpectators::backcolor" "0.6 1 1 0" ;
		set "VoteTypeKick::backcolor" "0.6 1 1 0" ;
		
		set "Main::noevents" "1" ;
		set "NewVote::noevents" "0" ;
		set "Multiplayer::noevents" "0" ;
		set "Controls::noevents" "0" ;
		set "Options::noevents" "0" ;
		set "System::noevents" "0" ;
		set "Exit::noevents" "0" ;
		
		set "Main::backcolor" "0 0.05 0 0.4" ;
		set "NewVote::backcolor" "0 0.05 0 0" ;
		set "Multiplayer::backcolor" "0 0.05 0 0" ;
		set "Controls::backcolor" "0 0.05 0 0" ;
		set "Options::backcolor" "0 0.05 0 0" ;
		set "System::backcolor" "0 0.05 0 0" ;
		set "Exit::backcolor" "0 0.05 0 0" ;
		if ( "gui::team" == 0 ) {
			set "TeamOn::redalpha" "0.4" ;
			set "TeamOn::bluealpha" "0" ;
		} else {
			set "TeamOn::redalpha" "0" ;
			set "TeamOn::bluealpha" "0.4" ;
		}
		
		// vote frame
		if ( "gui::isLastMan" == 0 ) {
			set "VoteTypeFragLimit::text" "#str_01982" ;
		} else {
			set "VoteTypeFragLimit::text" "#str_02483" ;
		}
		if ( "gui::callvote" == 1 ) {
			set "NewVote::noevents" "0" ;
			set "NewVote::forecolor" "0.698,1,1,1" ;
			set "NewVote::text" "#str_02484" ;
			if ( "gui::vote0" == 1 ) {
				set "VoteTypeRestart::noevents" "0" ;
				set "VoteTypeRestart::forecolor" "1,1,1,0.6" ;
			} else {
				set "VoteTypeRestart::noevents" "1" ;
				set "VoteTypeRestart::forecolor" "0.8,0.8,0.8,0.4" ;
			}
			if ( "gui::vote1" == 1 ) {
				set "VoteTypeTimelimit::noevents" "0" ;
				set "VoteTypeTimelimit::forecolor" "1,1,1,0.6" ;
			} else {
				set "VoteTypeTimelimit::noevents" "1" ;
				set "VoteTypeTimelimit::forecolor" "0.8,0.8,0.8,0.4" ;
			}
			if ( "gui::vote2" == 1 ) {
				set "VoteTypeFragLimit::noevents" "0" ;
				set "VoteTypeFragLimit::forecolor" "1,1,1,0.6" ;
			} else {
				set "VoteTypeFragLimit::noevents" "1" ;
				set "VoteTypeFragLimit::forecolor" "0.8,0.8,0.8,0.4" ;
			}
			if ( "gui::vote3" == 1 ) {
				set "VoteTypeGameType::noevents" "0" ;
				set "VoteTypeGameType::forecolor" "1,1,1,0.6" ;
			} else {
				set "VoteTypeGameType::noevents" "1" ;
				set "VoteTypeGameType::forecolor" "0.8,0.8,0.8,0.4" ;			
			}
			if ( "gui::vote4" == 1 ) {
				set "VoteTypeKick::noevents" "0" ;
				set "VoteTypeKick::forecolor" "1,1,1,0.6" ;
			} else {
				set "VoteTypeKick::noevents" "1" ;
				set "VoteTypeKick::forecolor" "0.8,0.8,0.8,0.4" ;
			}
			if ( "gui::vote5" == 1 ) {
				set "VoteTypeMap::noevents" "0" ;
				set "VoteTypeMap::forecolor" "1,1,1,0.6" ;				
			} else {
				set "VoteTypeMap::noevents" "1" ;
				set "VoteTypeMap::forecolor" "0.8,0.8,0.8,0.4" ;							
			}
			if ( "gui::vote6" == 1 ) {
				set "VoteTypeSpectators::noevents" "0" ;
				set "VoteTypeSpectators::forecolor" "1,1,1,0.6" ;
			} else {
				set "VoteTypeSpectators::noevents" "1" ;
				set "VoteTypeSpectators::forecolor" "0.8,0.8,0.8,0.4" ;
			}
			if ( "gui::vote7" == 1 ) {
				set "VoteTypeNextMap::noevents" "0" ;
				set "VoteTypeNextMap::forecolor" "1,1,1,0.6" ;
			} else {
				set "VoteTypeNextMap::noevents" "1" ;
				set "VoteTypeNextMap::forecolor" "0.8,0.8,0.8,0.4" ;
			}
		} else {
			set "NewVote::noevents" "1" ;
			set "NewVote::forecolor" "1,1,1,0.4" ;
			if ( "gui::callvote" == 0 ) {
				set "NewVote::text" "#str_04147" ;
			} else {
				set "NewVote::text" "#str_04148" ;
			}
		}
		// hide all vote value things
		set "valueType" "99" ;
	}

	onEvent {
		if ( "valueType" != 0 ) {
			
			// clear everything
			//set "ValueTypeMessage::rect" "171,2,200,14" ;
			set "ValueTypeMessage::visible" "0" ;
			set "ValueTypeEdit::visible" "0" ;
			set "ValueTypeMapShot::visible" "0" ;
			set "ValueTypeMapVal::visible" "0" ;
			set "ValueTypeChoices::visible" "0" ;
			set "CallVote::visible" "0" ;
			// setup the appropriate valueType
			if ( "valueType" == 1 ) {
				if ( "localVoteIndex" == 0 ) {
					set "ValueTypeMessage::text" "#str_02487" ;
				//	set "ValueTypeMessage::rect" "171,18,200,14" ;
				}
				if ( "localVoteIndex" == 6 ) {
					if ( "gui::si_spectators" == 0 ) {
						set "ValueTypeMessage::text" "#str_02300" ;
					} else {
						set "ValueTypeMessage::text" "#str_02488" ;
					}
				//	set "ValueTypeMessage::rect" "171,18,200,14" ;
				}
				if ( "localVoteIndex" == 7 ) {
					set "ValueTypeMessage::text" "#str_04149" ;
				//	set "ValueTypeMessage::rect" "171,18,200,14" ;
				}
				set "ValueTypeMessage::visible" "1" ;
				set "CallVote::visible" "1" ;
			}
			if ( "valueType" == 2 ) {
				if ( "localVoteIndex" == 1 ) {
					set "ValueTypeMessage::text" "#str_02489" ;
					set "ValueTypeEdit::text" "gui::si_timeLimit" ;
				}
				if ( "localVoteIndex" == 2 ) {
					if ( "gui::isLastMan" == 0 ) {
						set "ValueTypeMessage::text" "#str_02490" ;
					} else {
						set "ValueTypeMessage::text" "#str_02491" ;
					}
					set "ValueTypeEdit::text" "gui::si_fragLimit" ;		
				}
				set "ValueTypeMessage::visible" "1" ;
				set "ValueTypeEdit::visible" "1" ;
				set "CallVote::visible" "1" ;
			}
			if ( "valueType" == 3 ) {
				if ( "localVoteIndex" == 3 ) {
					set "ValueTypeMessage::text" "#str_02492" ;
					set "ValueTypeChoices::choices" "Deathmatch;Tourney;Team DM;Last Man" ;
				}
				if ( "localVoteIndex" == 4 ) {
					set "ValueTypeMessage::text" "#str_02493" ;
					set "ValueTypeChoices::choices" "gui::kickChoices" ;
				}					
				set "ValueTypeMessage::visible" "1" ;
				set "ValueTypeChoices::visible" "1" ;
				set "CallVote::visible" "1" ;
			}
			if ( "valueType" == 4 ) {
				set "ValueTypeMessage::text" "#str_02494" ;
				set "ValueTypeMessage::visible" "1" ;
				set "ValueTypeMapShot::visible" "1" ;
				set "ValueTypeMapVal::visible" "1" ;
				set "CallVote::visible" "1" ;
			}
			set "valueType" "0" ;
		}
	}

	onESC {
		set "cmd" "close" ;
	}

