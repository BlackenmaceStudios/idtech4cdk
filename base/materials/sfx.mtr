/* this flare uses global parms to affect the color of all using this shader */
textures/sfx/flare_global
{
	noshadows
	translucent
	nonsolid
	deform flare 		16
	qer_editorimage		textures/editor/flare_global.tga
	{
		if ( parm7 == 0 )
		blend		add
		map			_quadratic
		red			parm0 * global0
		green		parm1 * global0
		blue		parm2 * global0
		vertexcolor
	}
}

