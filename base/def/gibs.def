
/***************************************************
(GIBS) Right Upper Arm Stub
***************************************************/

export fred {
	options -prefix GIB1_
	mesh	models/gibs/cycles/initial.mb -dest models/md5/gibs/ruparmstubgib
	anim	models/gibs/cycles/initial.mb -dest models/md5/gibs/ruparmstubgib
}

model env_gibs_ruparmstub {
	mesh							models/md5/gibs/ruparmstubgib.md5mesh
	anim af_pose					models/md5/gibs/ruparmstubgib.md5anim
	anim idle						models/md5/gibs/ruparmstubgib.md5anim
	anim initial					models/md5/gibs/ruparmstubgib.md5anim
}

entityDef env_gibs_ruparmstub {
	"editor_color"					"1 .5 0"
	"editor_mins"					"-8 -8 -8"
	"editor_maxs"					"8 8 8"

	"editor_usage"					"Right Upper Arm Stub Gib."

	"spawnclass"					"idAFEntity_Generic"
	"snd_bounce"					"bodyparts"

	"model"							"env_gibs_ruparmstub"
	"articulatedFigure"				"env_gibs_ruparmstub.af"
}

