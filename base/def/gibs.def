/***************************************************
ragdoll skeleton
***************************************************/

export fred {
	options		-prefix SKEL_ -align ALL -parent Loneck Shoulders  -sourcedir models/monsters/skeleton/cycles -destdir models/md5/monsters/skeleton
	mesh		skeleton_mesh.mb -dest skeleton
	anim		idle.mb
	anim		initial.mb
}

model env_ragdoll_skeleton {
	mesh							models/md5/monsters/skeleton/skeleton.md5mesh
	skin 							skins/monsters/skeleton/skragdoll.skin
	anim af_pose					models/md5/monsters/skeleton/initial.md5anim
	anim initial					models/md5/monsters/skeleton/initial.md5anim
	anim idle						models/md5/monsters/skeleton/idle.md5anim
}

entityDef env_ragdoll_skeleton {
	"editor_color"					"1 .5 0"
	"editor_mins"					"-8 -8 -8"
	"editor_maxs"					"8 8 8"
	"editor_ragdoll"				"1"	

	"editor_usage"					"Ragdoll for skeleton"

	"spawnclass"					"idAFEntity_Generic"
	"bleed"							"1"
	"sleep"							"1"

	"model"							"env_ragdoll_skeleton"
	"articulatedFigure"				"env_skeleton"
}

model env_ragdoll_skeleton2 {
	inherit							env_ragdoll_skeleton
	skin 							skins/monsters/skeleton/skragdoll2.skin
}

entityDef env_ragdoll_skeleton2 {
	"inherit"						"env_ragdoll_skeleton"

	"model"							"env_ragdoll_skeleton2"
}